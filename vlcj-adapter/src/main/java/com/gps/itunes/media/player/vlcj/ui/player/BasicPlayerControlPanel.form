<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.gps.itunes.media.player.vlcj.ui.player.BasicPlayerControlPanel">
  <grid id="27dc6" binding="panel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <maximumSize width="200" height="30"/>
      <preferredSize width="300" height="60"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="a22d2" binding="wrapperPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="7fac5" binding="iconsPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="84afc" class="javax.swing.JLabel" binding="previousIcon">
                <constraints/>
                <properties>
                  <icon value="icons/previous.png"/>
                  <text value=""/>
                </properties>
              </component>
              <component id="c406c" class="javax.swing.JLabel" binding="playIcon">
                <constraints/>
                <properties>
                  <icon value="icons/play.png"/>
                  <text value=""/>
                </properties>
              </component>
              <component id="9c43" class="javax.swing.JLabel" binding="forwardIcon">
                <constraints/>
                <properties>
                  <icon value="icons/forward.png"/>
                  <text value=""/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="b816" binding="volumePanel" layout-manager="FlowLayout" hgap="0" vgap="5" flow-align="1">
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="3017b" class="javax.swing.JSlider" binding="volumeSlider" custom-create="true">
                <constraints/>
                <properties>
                  <maximum value="200"/>
                  <paintLabels value="false"/>
                  <paintTicks value="false"/>
                  <paintTrack value="true"/>
                  <snapToTicks value="false"/>
                  <value value="75"/>
                </properties>
              </component>
              <component id="24e82" class="javax.swing.JLabel" binding="volumeValueLabel" custom-create="true">
                <constraints/>
                <properties>
                  <horizontalAlignment value="0"/>
                  <horizontalTextPosition value="0"/>
                  <text value=""/>
                  <toolTipText resource-bundle="vlcjAdapterUi" key="volume.label.tooltip"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
